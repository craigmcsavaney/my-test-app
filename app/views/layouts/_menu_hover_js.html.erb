<script type="text/javascript">
  $(document).ready(function() {
    $('.cb-top-menu').on('mouseenter', function() {
      $(this).addClass("cb-navbar-top-menu-item-hover");
    });

    $('.cb-top-menu').on('mouseleave', function() {
      $(this).removeClass("cb-navbar-top-menu-item-hover");
    });
  });
</script>
