<% provide(:title, 'Serve') %>
<h1>Serve</h1>

<div class="container">
	<ul class="class-index">
      	<%= render @serve %>
	</ul>
	<br/>
      <%= form_for(@serve) do |f| %>
        <%= f.label :email %>
        <%= f.text_field :email %>

        <%= f.submit "add or update email", class: "btn btn-large btn-primary" %>
      <% end %>
	<%= go_back %>
</div>