<li>
	<% @editpath = edit_serve_path(serve) %>
  	<b><%= :id %>: </b><%= link_to serve.id, @editpath %><br/>
  	<b><%= :promotion_id %>: </b><%= link_to serve.promotion_id, @editpath %><br/>
  	<b><%= :email %>: </b><%= link_to serve.email, @editpath %><br/>
  	<b><%= :created_at %>: </b><%= link_to serve.created_at, @editpath %><br/>
  	<b><%= :updated_at %>: </b><%= link_to serve.updated_at, @editpath %><br/>
	<br/>
  	<b>Actions: </b><%= link_to "edit", @editpath %>&nbsp;&nbsp;
  	<%= link_to "delete", serve, method: :delete,
                                  data: { confirm: "You sure?" } %>
</li>